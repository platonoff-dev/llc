let map = fn(arr, f) {
    let iter = fn(arr, accumulated) {
        if (len(arr) == 0) {
            return accumulated
        } else {
            return iter(rest(arr), push(accumulated, f(first(arr))))
        }
    }
    return iter(arr, [])
}

let reduce = fn(arr, initial, f) {
    let iter = fn(arr, result) {
        if (len(arr) == 0) {
            return result
        } else {
            iter(rest(arr), f(result, first(arr)))
        }
    }
    return iter(arr, initial)
}
